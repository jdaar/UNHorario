    "@fullcalendar/timegrid": "^5.11.3",
    "@fullcalendar/vue3": "^5.11.2",
    "@types/file-saver": "^2.0.5",
    "bootstrap-icons": "^1.9.1",
    "datebook": "^7.0.8",
    "file-saver": "^2.0.5",
    "firebase": "^9.15.0",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-select": "^3.20.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
